\begin_layout Enumerate
Se pide estimar los primeros 128 valores de la autocorrelacion de la secuencia
 aleatoria X(n) adjunta con la consigna.
\end_layout

\begin_deeper
\begin_layout Standard
La estimacion se realizo con dos estimadores distintos dados por las siguientes
 formulas:
\end_layout

\begin_layout Standard
\align center
Estimador no polarizdo:
\begin_inset Formula 
\begin{equation}
\hat{R}_{np}(k)=\hat{R}_{np}(-k)=\frac{1}{N-k}\stackrel[i=0]{N-k-1}{\sum}X(i)X(i+k),\forall k<N
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\align center
Estimador polarizado:
\end_layout

\begin_layout Standard
\align center
\begin_inset Formula 
\begin{equation}
\hat{R}_{p}(k)=\hat{R}_{p}(k)=\frac{1}{N}\stackrel[i=0]{N-k-1}{\sum}X(i)X(i+k),\forall k<N
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\align center
\begin_inset Formula 
\[
\hat{R}_{p}(k)=\hat{R}_{np}(k)=0,\forall k\geqq N
\]

\end_inset


\end_layout

\begin_layout Standard
Se puede ver facilmente que la diferencia entre ambos estimadores esta dado
 por una constante segun:
\end_layout

\begin_layout Standard
\align center
\begin_inset Formula 
\[
\hat{R}_{p}(k)=\frac{1}{N}\stackrel[i=0]{N-k-1}{\sum}X(i)X(i+k)=\frac{1}{N}.\frac{N-k}{N-k}.\stackrel[i=0]{N-k-1}{\sum}X(i)X(i+k)=\frac{N-k}{N}\hat{R}_{np}(k)
\]

\end_inset


\end_layout

\begin_layout Standard
\align center
\begin_inset Formula 
\begin{equation}
\hat{R}_{p}(k)=\frac{N-k}{N}\hat{R}_{np}(k)
\end{equation}

\end_inset


\end_layout

